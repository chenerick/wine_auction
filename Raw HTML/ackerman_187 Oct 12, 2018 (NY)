    <div data-role="popup" id="actonForm" class="ui-content">
    </div>

    <script src="/bundles/js?v=lugpLJF7fdrxwnoD66sR4xlDWCjapVvfoDHKIoWC05E1"></script>






	<script src="/Scripts/views/auctionarchives/index.js?v=zok1xmzk1x2"></script>


    <script src="/Scripts/views/end.js"></script>

    <script src="/Scripts/jquery-ui-1.12.1.min.js"></script>

    <script type="text/javascript">
        $("#join-mailing-list").keyup(function (event) {
            if (event.keyCode == 13) {
                $("#join-mailing-list-btn").click();
            }
        });

        function onSubscribeEmail() {
            var email = $('#join-mailing-list').val();
            $("#join-mailing-list-btn").addClass("primary-button-loader-small");
            $('#join-mailing-list').attr('disabled', 'disabled');

            acker.api.emailSubscribe(email,
                (function (data, success) {
                    if (success && data === true) {
                        pageNotifier.showPageNotification(msgMailingListSubscribeSuccess, 'warning', true, 4000);
                        $('#join-mailing-list').val("");
						showActOnForm(email);
                    } else {
                        pageNotifier.showPageNotification(msgMailingListSubscribeFailure, 'error', true, 4000);
                    }
                    $('#join-mailing-list').removeAttr('disabled');
                    $("#join-mailing-list-btn").removeClass("primary-button-loader-small");
                }));
        };
    </script>
        <script type="text/javascript">

            var _gaq = _gaq || [];
            _gaq.push(['_setAccount', 'UA-45804359-1']);
            _gaq.push(['_setDomainName', 'ackerwines.com']);
            _gaq.push(['_trackPageview']);

            // Second tracker
            //_gaq.push(['secondTracker._setAccount', 'UA-9279786-1']);
            //_gaq.push(['secondTracker._trackPageview']);
            //_gaq.push(['secondTracker._setDomainName', 'ackerwines.com']);


            (function () {
                var ga = document.createElement('script');
                ga.type = 'text/javascript';
                ga.async = true;
                ga.src =
                    ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                var s = document.getElementsByTagName('script')[0];
                s.parentNode.insertBefore(ga, s);
            })();

        </script>
        <script>
            /*<![CDATA[*/
            (function (w, a, b, d, s) {
                w[a] = w[a] || {};
                w[a][b] =
                    w[a][b] || { q: [], track: function (r, e, t) { this.q.push({ r: r, e: e, t: t || +new Date }); } };
                var e = d.createElement(s);
                var f = d.getElementsByTagName(s)[0];
                e.async = 1;
                e.src = '//marketing.ackerwines.com/cdnr/34/acton/bn/tracker/35823';
                f.parentNode.insertBefore(e, f);
            })(window, 'ActOn', 'Beacon', document, 'script');
            ActOn.Beacon.track(); /*]]>*/
        </script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('.newsroom-articles a').each(function () {
                var a = new RegExp('/' + window.location.host + '/');
                if (!a.test(this.href)) {
                    $(this).click(function (event) {
                        event.preventDefault();
                        event.stopPropagation();
                        window.open(this.href, '_blank');
                    }
                    );
                }
            }
            );
        }
        );
    </script>
    <script>
        $(document).ready(function () {
            $("#actonForm").dialog({
                autoOpen: false,
                modal: true,
                height: 760,
                width: 600,
                resizable: false,
                title: '',
                clickOutside: true
            });
        });

        function showActOnForm(email) {
            $("#actonForm").html('<iframe id="actOnIframe" width="100%" height="100%"        marginWidth="0" marginHeight="0" frameBorder="0" scrolling="auto" />').dialog("open");
			$("#actOnIframe").attr("src", "https://marketing.ackerwines.com/acton/fs/blocks/showLandingPage/a/35823/p/p-0009/t/page/fm/0?email=" + encodeURIComponent(email)); return false;
        }

        function closeActOnModal() {
            $("#actonForm").dialog("close");
        }

        function receiveMessage(event) {
            if (event.origin !== "https://marketing.ackerwines.com")
                return;

            closeActOnModal();
        }
        window.addEventListener("message", receiveMessage, false);

        $("a[data-popup]").live('click', function (e) {
            window.open($(this)[0].href, '_blank');
            e.preventDefault();
        });
    </script>
</body>
